<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageVersion>1.6.0</PackageVersion>
        <Title>Jupiter</Title>
        <Authors>Akaion</Authors>
        <Description>A Windows virtual memory editing library with support for pattern scanning.</Description>
        <RepositoryUrl>https://github.com/Akaion/Jupiter</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\Jupiter.xml</DocumentationFile>
    </PropertyGroup>

</Project>
